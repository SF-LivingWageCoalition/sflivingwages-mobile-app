# .env.example â€” copy to .env and fill values (do NOT commit secrets)

## Environment Variables for the Authentication and WooCommerce APIs
# Base URL for the APIs (e.g. the website domain)
EXPO_PUBLIC_BASE_URL=https://staging.example.com
# JWT Route for authentication
EXPO_PUBLIC_JWT_ROUTE=/wp-json/simple-jwt-login/v1
# WooCommerce API Route
EXPO_PUBLIC_WC_ROUTE=/wp-json/wc/v3
# JWT Decryption Key / Algorithm / Token Type
EXPO_PUBLIC_JWT_DE_KEY=<decryption_key_here>
EXPO_PUBLIC_JWT_DE_ALG=HS256
EXPO_PUBLIC_JWT_TYP=JWT
# JWT Auth Key
EXPO_PUBLIC_JWT_AUTH_KEY=<auth_key_here>
# WooCommerce REST API credentials
EXPO_PUBLIC_CONSUMER_KEY=<woo_consumer_key>
EXPO_PUBLIC_CONSUMER_SECRET=<woo_consumer_secret>
# Fetch timeout in milliseconds
EXPO_PUBLIC_FETCH_TIMEOUT_MS=10000

## Environment Variables for the Violation Reporting Feature
# Google Places Autocomplete API Key
EXPO_PUBLIC_GOOGLE_AUTOCOMPLETE_API_KEY=<google_places_api_key>
