{
  "expo": {
    "name": "sflivingwages",
    "slug": "sflw",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "11.0.4",
    "orientation": "portrait",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.sfLiving.sflivingwages",
      "buildNumber": "2.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#d31623"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.sfLiving.sflivingwagesappbeta",
      "versionCode": 17
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#d31623"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-localization",
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "50c24863-5dd8-46ec-be8e-8e121b0c3775"
      }
    }
  }
}
